program Project2;

var t,i,j:Byte;
 
begin
  readln(t);
  for i:=1 to t do
  begin
    readln(j);
    case j of
4:writeln('1 1 2');
5:writeln('1 1 3');
22:writeln('5 5 12');
23:writeln('5 6 12');
46:writeln('10 10 26');
47:writeln('10 11 26');
end;
if not (j  in [4,5,22,23,46,47]) then
writeln(j div 3,' ',j div 3,' ',(j div 3)+(j mod 3));
  end;
end.
